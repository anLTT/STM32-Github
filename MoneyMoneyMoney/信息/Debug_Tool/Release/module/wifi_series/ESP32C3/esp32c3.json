      {
        //命令，超时，延时，重试次数
        "AT_CMD": [ "AT+CIPAP?\r\n", 2000, 0, 1 ],
        //响应值，数据头,数据尾,多条响应,是否异步
        "AT_RESPONSE": [ "+CIPAP:ip:{@IP}\r\n+CIPAP:gateway:{@GATEWAY}\r\n+CIPAP:netmask:{@NETMASK}\r\n{@ELSE}OK\r\n", "+CIPAP:ip", "OK\r\n", false, false ]
